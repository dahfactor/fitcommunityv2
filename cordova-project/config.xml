<?xml version='1.0' encoding='utf-8'?>
<widget id="com.miamicoder.bookit" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FitCommunity</name>
    <description>
        
    </description>
    <author email="miamicoder@gmail.com" href="http://miamicoder.com">
        
    </author>
    <content src="www/index.html" />

	<preference name='phonegap-version' value='cli-5.1.1' />
    <preference name="StatusBarOverlaysWebView" value="false"></preference>
    <preference name="StatusBarBackgroundColor" value="#662d91"></preference>
	
	<gap:plugin name="cordova-plugin-whitelist" source="npm" />
	
	<!-- Allow links to example.com -->
	<allow-navigation href="http://dfactor.impactsw.com/app/*" />
	
	<!-- Allow links to example.com to open in a browser -->
	<allow-intent href="http://dfactor.impactsw.com/app/*" />
	
	<access origin="*" />
</widget>
