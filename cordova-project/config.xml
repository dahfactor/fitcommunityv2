<?xml version='1.0' encoding='utf-8'?>
<widget id="com.miamicoder.bookit" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FitCommunity</name>
    <description>
        
    </description>
    <author email="miamicoder@gmail.com" href="http://miamicoder.com">
        
    </author>
    <content src="/www/index.html" />
    <access origin="*" />
	<access origin="http://fit.comuf.com" />

    <preference name="StatusBarOverlaysWebView" value="false"></preference>
    <preference name="StatusBarBackgroundColor" value="#662d91"></preference>
	
	<gap:plugin name="org.apache.cordova-plugin-whitelist" source="npm" />
	
	<plugin name="cordova-plugin-whitelist" version="1" />
	<allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
